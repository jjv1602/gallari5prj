{
    "version": 2,
    "builds": [
        {
            "src": "./Backend/index.js",
            "use": "@vercel/node"
        },
        {
            "src": "frontend/dist", 
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/[^.]+",
            "dest": "/"
        }
    ]
}